{
  "source": {
    "include": ["."],
    "exclude": ["./node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm"
  }
}
